package ShowbizPeople;

public class Follower extends AbstractPeople2 {	
	
	protected void followKol(KOL kolToFollow) {
		System.out.println(this.getName() + " Ä‘ang follow Tiktoker " + kolToFollow.getName());
		kolToFollow.saveFollowers(this);
	}
	
	protected void receiveNoti(KOL kolToFollow) {		
			System.out.println(this.getName()+ " have received a notification from " + kolToFollow.getName());			
	}
	

}
